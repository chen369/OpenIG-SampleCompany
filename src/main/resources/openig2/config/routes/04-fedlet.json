{
  "handler": {
    "type": "SamlFederationHandler",
    "config": {
      "assertionMapping": {
        "mail": "mail",
        "name": "name"
      },
      "subjectMapping": "subjectName",
      "redirectURI": "http://travel-ig.sample.com:9002/travelApp"
    },
    "capture": "all"
  },
  "condition": "${matches(request.uri.path, '^/saml')}",
  "session": "JwtSession"
}



