{
  "baseURI": "http://travel.sample.com:8012",
  "handler": {
    "type": "DispatchHandler",
    "config": {
      "bindings": [
        {
          "condition": "${empty session.mail}",
          "handler": {
            "type": "StaticResponseHandler",
            "config": {
              "status": 302,
              "reason": "Found",
              "headers": {
                "Location": [
                  "http://travel-ig.sample.com:9002/saml/SPInitiatedSSO"
                ]
              }
            },
            "capture": "all"
          }
        },
        {
          "handler": "ClientHandler"
        }
      ]
    }
  },
  "condition": "${matches(request.uri.path, '^/travelApp')}",
  "session": "JwtSession"
}

